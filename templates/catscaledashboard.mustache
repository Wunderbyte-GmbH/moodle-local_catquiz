<ul class="nav nav-tabs" id="myTab" role="tablist">
  <li class="nav-item">
    <a class="nav-link active" id="testitems-tab" data-toggle="tab" href="#testitems" role="tab" aria-controls="testitems" aria-selected="true">{{#str}} testitems, local_catquiz{{/str}}</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" id="addtestitems-tab" data-toggle="tab" href="#addtestitems" role="tab" aria-controls="addtestitems" aria-selected="false">{{#str}} addtestitem, local_catquiz{{/str}}</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" id="contact-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">Contact</a>
  </li>
</ul>
<div class="tab-content" id="myTabContent">
  <div class="tab-pane fade show active" id="testitems" role="tabpanel" aria-labelledby="testitems-tab">
    {{{testitemstable}}}
  </div>
  <div class="tab-pane fade" id="addtestitems" role="tabpanel" aria-labelledby="addtestitems-tab">
    {{{addtestitemstable}}}
  </div>
  <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">
  </div>
</div>