{{!
    @template local_catquiz
    Template which defines a catquiz view page.
    Classes required for JS:
    * none
    Data attributes required for JS:
    * none
    Context variables required for this template:
    Example context (json):
    {
      "contextselector": "Contextselector HTML",
      "modelbutton": "Modelbutton HTML"
    }
}}
<p>
  <a class="btn btn-primary" href="{{{returnurl}}}">{{#str}} managecatscales, local_catquiz {{/str}}</a>
  {{{contextselector}}}
  {{{modelbutton}}}
</p>


<ul class="nav nav-tabs" id="myTab" role="tablist">
  <li class="nav-item">
    <a class="nav-link active" id="statistics-tab" data-toggle="tab" href="#statistics" role="tab" aria-controls="statistics" aria-selected="true">{{#str}} statistics, local_catquiz{{/str}}</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" id="testitems-tab" data-toggle="tab" href="#testitems" role="tab" aria-controls="testitems" aria-selected="false">{{#str}} testitems, local_catquiz{{/str}}</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" id="usage-tab" data-toggle="tab" href="#usage" role="tab" aria-controls="usage" aria-selected="false">{{#str}} usage, local_catquiz{{/str}}</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" id="students-tab" data-toggle="tab" href="#studentstats" role="tab" aria-controls="studentstats" aria-selected="false">{{#str}} studentstats, local_catquiz{{/str}}</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" id="filepicker-tab" data-toggle="tab" href="#filepicker" role="tab" aria-controls="filepicker" aria-selected="false">{{#str}} import, core{{/str}}</a>
  </li>
</ul>
<div class="tab-content" id="myTabContent">
  <div class="tab-pane fade show active" id="statistics" role="tabpanel" aria-labelledby="statistics-tab">
    {{>local_catquiz/tabs/tab_statistics}}
  </div>
  <div class="tab-pane fade" id="testitems" role="tabpanel" aria-labelledby="testitems-tab">
    {{{testitemstable}}}
  </div>
  <div class="tab-pane fade" id="usage" role="tabpanel" aria-labelledby="usage-tab">
    {{{table}}}
  </div>
  <div class="tab-pane fade" id="studentstats" role="tabpanel" aria-labelledby="students-tab">
    {{{studentstable}}}
  </div>
  <div class="tab-pane fade" id="filepicker" role="tabpanel" aria-labelledby="filepicker-tab">
    {{{testitemsimporter}}}
    {{>local_catquiz/importer/importerdatainfodisplay}}
  </div>
</div>

{{#js}}
    require(['local_catquiz/catscaledashboard'], function(init) {
        init.init();
    });
    require(['local_catquiz/csvimport'], function(init) {
      init.init();
  });
{{/js}}
